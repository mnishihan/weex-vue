{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "label": "Project name"
    },
    "description": {
      "type": "string",
      "required": true,
      "label": "Project description",
      "default": "A Weex project"
    },
    "appId": {
      "type": "string",
      "required": true,
      "label": "App id / bundle id",
      "default": "com.your_company.project"
    },
    "author": {
      "type": "string",
      "label": "Author"
    }
  },
  "skipInterpolation": "**/*.{vue,js},src/**/*.html",
  "completeMessage": "To get started:\n\n  cd {{destDirName}}\n  npm install\n  npm run build\n  npm run serve\n  open http://localhost:8080/"
}
